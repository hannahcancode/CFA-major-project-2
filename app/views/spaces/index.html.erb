    <% @spaces.each do |space| %>
      <div class="box">
        <p><%= space.name %></p>
        <p><%= space.description %></p>
        <p><%= space.street_number %> <%= space.street_name %></p>
        <p><%= space.suburb %> <%= space.postal_code %></p>
        <p><%= space.state %> <%= space.country %></p>
        <p><%= space.user.email %></p>
        <p><%= link_to 'Show', space %> <%= link_to 'Edit', edit_space_path(space) %> <%= link_to 'Destroy', space, method: :delete, data: { confirm: 'Are you sure?' } %></p>
